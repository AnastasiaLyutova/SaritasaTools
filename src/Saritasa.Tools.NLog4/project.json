{
  "title": "Saritasa Tools for NLog4",
  "version": "0.1.1",
  "authors": [ "Saritasa" ],
  "description": "The package is a part of Saritasa Tools project. Saritasa.Tools.NLog4 contains implementation for Microsoft common logging infrastructure for NLog.",
  "copyright": "Saritasa 2015-2017",
  "packOptions": {
    "iconUrl": "https://raw.githubusercontent.com/Saritasa/SaritasaTools/master/icon.png",
    "licenseUrl": "https://raw.githubusercontent.com/Saritasa/SaritasaTools/master/LICENSE",
    "projectUrl": "https://github.com/Saritasa/SaritasaTools",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/Saritasa/SaritasaTools"
    },
    "summary": "NLog4 implementation for Microsoft common logging infrastructure.",
    "tags": [ "nlog", "logging" ]
  },
  "frameworks": {
    "net452": {
      "dependencies": {
        "NLog": "4.4.2"
      }
    },
    "net461": {
      "dependencies": {
        "NLog": "4.4.2"
      }
    },
    "netcoreapp1.0": {
      "dependencies": {
        "NLog": "5.0.0-beta05",
        "Microsoft.NETCore.App": "1.1.0",
        "System.Threading": "4.3.0"
      }
    },
    "netstandard1.6": {
      "dependencies": {
        "NLog": "5.0.0-beta05",
        "NETStandard.Library": "1.6.1",
        "System.Threading": "4.3.0"
      }
    }
  },
  "dependencies": {
    "Microsoft.Extensions.Logging.Abstractions": "1.1.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    },
    "JetBrains.Annotations": {
      "version": "10.2.1",
      "type": "build"
    }
  },
  "buildOptions": {
    "keyFile": "../../tools/SaritasaTools.snk",
    "additionalArguments": [ "/ruleset:../../tools/Saritasa.ruleset" ],
    "xmlDoc": true
  }
}
